{
    "plugins": [
      "cypress",
      "no-only-tests"
    ],
    "rules": {
        "cypress/no-assigning-return-values": "error",
        "cypress/no-unnecessary-waiting": "error",
        "no-only-tests/no-only-tests": "error",
        "quotes": [2, "single", { "avoidEscape": true }]
      },
      "env": {
        "cypress/globals": true
      }
  }